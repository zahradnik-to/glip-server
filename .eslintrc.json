{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "end_of_line": "off",
        "semi": "warn",
        "no-underscore-dangle": "off",
        "padded-blocks": "warn",
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "no-console": "off",
        "max-len": "off"
    }
}
