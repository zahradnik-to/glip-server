{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "end_of_line": "off",
        "semi": "warn",
        "no-underscore-dangle": "off"
    }
}
